<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="45.0" prefWidth="885.0" style="-fx-background-color: ffffff;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.my_group_project.AdminHomePaneController">
   <children>
      <Pane prefHeight="45.0" prefWidth="75.0">
         <children>
            <Label fx:id="bookID" alignment="CENTER" layoutX="2.0" layoutY="13.0" prefHeight="19.0" prefWidth="71.0" text="BOOK ID">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane prefHeight="45.0" prefWidth="242.0">
         <children>
            <Label fx:id="nameOfBook" alignment="CENTER" contentDisplay="CENTER" layoutX="12.0" layoutY="12.0" prefHeight="21.0" prefWidth="217.0" text="NAME OF BOOK">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane prefHeight="45.0" prefWidth="194.0">
         <children>
            <Label fx:id="author" alignment="CENTER" contentDisplay="CENTER" layoutX="19.0" layoutY="13.0" prefHeight="19.0" prefWidth="157.0" text="AUTHOR">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane prefHeight="45.0" prefWidth="147.0">
         <children>
            <Label fx:id="category" alignment="CENTER" contentDisplay="CENTER" layoutX="17.0" layoutY="13.0" prefHeight="19.0" prefWidth="114.0" text="CATEGORY">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane prefHeight="45.0" prefWidth="171.0">
         <children>
            <Label fx:id="quantity" alignment="CENTER" contentDisplay="CENTER" layoutX="50.0" layoutY="13.0" text="QUANTITY">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Button fx:id="editButton" mnemonicParsing="false" prefHeight="45.0" prefWidth="57.0" style="-fx-background-color: transparent; -fx-border-color: transparent;">
         <graphic>
            <ImageView fitHeight="34.0" fitWidth="30.0">
               <image>
                  <Image url="@../../../picture/edit.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</HBox>
