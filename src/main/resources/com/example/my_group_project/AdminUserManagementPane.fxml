<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<HBox fx:id="hbox" prefHeight="45.0" prefWidth="885.0" style="-fx-background-color: ffffff;" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane prefHeight="45.0" prefWidth="130.0">
         <children>
            <Label fx:id="userId" alignment="CENTER" layoutX="36.0" layoutY="13.0" text="USER ID">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane prefHeight="45.0" prefWidth="150.0">
         <children>
            <Label fx:id="userName" alignment="CENTER" contentDisplay="CENTER" layoutX="34.0" layoutY="14.0" text="USER NAME">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane prefHeight="45.0" prefWidth="154.0">
         <children>
            <Label fx:id="email" alignment="CENTER" layoutX="60.0" layoutY="14.0" text="EMAIL">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane prefHeight="45.0" prefWidth="140.0">
         <children>
            <Label fx:id="phoneNumber" alignment="CENTER" layoutX="13.0" layoutY="13.0" text="PHONE NUMBER">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane prefHeight="45.0" prefWidth="160.0">
         <children>
            <Label fx:id="dateOfBirth" alignment="CENTER" layoutX="30.0" layoutY="14.0" text="DATE OF BIRTH">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane prefHeight="45.0" prefWidth="140.0">
         <children>
            <Label fx:id="gender" alignment="CENTER" layoutX="42.0" layoutY="14.0" text="GENDER">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Button fx:id="moreButton" mnemonicParsing="false" prefHeight="44.0" prefWidth="40.0" style="-fx-background-color: transparent; -fx-border-color: transparent;">
         <graphic>
            <ImageView fitHeight="34.0" fitWidth="30.0">
               <image>
                  <Image url="@../../../picture/edit.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</HBox>
